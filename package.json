	},
	"activationEvents": [
		"onCommand:python-image-preview.pythonImagePreview"
	],
	"main": "./dist/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "python-image-preview.pythonImagePreview",
				"title": "Python Image Preview"
